{{> organisms-header }}
<main role="main">

	<div class="c-page-header l">
		<h1 class="c-page-header__title">{{ pageTitle }}</h1>
	</div>

	<div class="l l--two-col">
		<div class="l-main">
			<section class="section">

				{{> organisms-text-blob }}

				{{# form }}
				{{> organisms-stacked-form }}
				{{/ form }}
			</section>
		</div><!--end .l-main-->

		<div class="l-sidebar">
			{{# latestPosts }}
			{{> organisms-section-media-list }}
			{{/ latestPosts}}
			{{# featuredPeople }}
			{{> organisms-section-media-list }}
			{{/ featuredPeople}}
		</div><!--end .l-sidebar-->
	</div><!--end l--two-col-->
</main>
{{> organisms-footer }}
